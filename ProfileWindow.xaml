        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Profile" Height="400" Width="500">
    <Grid>
        <DockPanel Margin="10" Name="pnlMain">
            <DockPanel.Resources>
            </DockPanel.Resources>
